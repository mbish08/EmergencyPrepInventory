<%= f.label :type, "Type of storage:" %>
<%= f.collection_select :type_id, Type.all, :id, :name, prompt: true %>
<%= f.fields_for :type do |t| %>
    <%= t.label :name, "Add a new storage type, if not listed: " %>
    <%= t.text_field :name %>
    <%= t.hidden_field :user_id, value: current_user.id %>
<% end %>
<br>